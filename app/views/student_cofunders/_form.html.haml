= form_for @student_cofunder do |f|
  - if @student_cofunder.errors.any?
    #error_explanation
      %h2= "#{pluralize(@student_cofunder.errors.count, "error")} prohibited this student_cofunder from being saved:"
      %ul
        - @student_cofunder.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
